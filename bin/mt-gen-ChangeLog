#!/usr/bin/env bash
(
    if [ -z "$1" ] ; then
        echo "$0: No out file specified."
        exit 1
    fi
    echo Generating "$1"
    source "$(dirname "$0")/mt-define-mousetrap-home"
    cd "$MOUSETRAP_HOME"
    echo "# Generated by `basename "$0"`" > "$1"
    echo "# Do not edit." >> "$1"
    git log --stat >> "$1"
)
