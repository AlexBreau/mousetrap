#!/usr/bin/env bash
(
    source "$(dirname "$0")/mt-define-mousetrap-home"
    cd "$MOUSETRAP_HOME"
    echo "# Generated by `basename $0`" > AUTHORS
    echo "# Do not edit." >> AUTHORS
    echo "Commits Author" >> AUTHORS
    git shortlog -s -e -n | sed 's/@/ AT /g' | sed 's/\./ DOT /g' >> AUTHORS
    )
